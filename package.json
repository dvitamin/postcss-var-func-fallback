{
  "name": "postcss-var-func-fallback",
  "version": "2.0.0",
  "description": "PostCSS plugin Adds fallback values to var(name) occurences.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "css-variables"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "dvitamin <dcoding@protonmail.com>",
  "license": "MIT",
  "repository": "dvitamin/postcss-var-func-fallback",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": ">=7.0.0"
  },
  "devDependencies": {
    "clean-publish": "^1.1.8",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "postcss": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "dependencies": {
    "postcss-value-parser": "^4.1.0"
  }
}
